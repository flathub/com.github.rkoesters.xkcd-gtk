{
	"app-id": "com.github.rkoesters.xkcd-gtk",
	"runtime": "org.freedesktop.Platform",
	"runtime-version": "18.08",
	"sdk": "org.freedesktop.Sdk",
	"sdk-extensions": [
		"org.freedesktop.Sdk.Extension.golang"
	],
	"command": "com.github.rkoesters.xkcd-gtk",
	"finish-args": [
		"--socket=x11",
		"--socket=wayland",
		"--share=ipc",
		"--share=network"
	],
	"modules": [
		{
			"name": "com.github.rkoesters.xkcd-gtk",
			"buildsystem": "simple",
			"build-options": {
				"env": {
					"GOPATH": "/nonexistent/go"
				}
			},
			"build-commands": [
				". /usr/lib/sdk/golang/enable.sh; make clean",
				". /usr/lib/sdk/golang/enable.sh; make",
				". /usr/lib/sdk/golang/enable.sh; make install prefix=/app"
			],
